{
	"$schema": "https://biomejs.dev/schemas/2.2.2/schema.json",
	"vcs": { "enabled": false, "clientKind": "git", "useIgnoreFile": false },
	"files": {
		"ignoreUnknown": false,
		"includes": [
			"src/**/*.js",
			"src/**/*.ts",
			"src/**/*.jsx",
			"src/**/*.tsx",
			"src/**/*.json"
		]
	},
	"formatter": { "enabled": true, "indentStyle": "tab" },
